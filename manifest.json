{
    "name": "TwitterRevertColoringExtensions",
    "description": "TwitterRevertColoringExtensions",
    "version": "1.1.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [{
        "matches": [
            "https://twitter.com/*"
        ],
        "css": [
            "css/TwitterRevertColoring.user.css",
            "css/TwitterRevertColoringPlus.user.css",
            "css/TwitterRevertColoringConfig.user.css"
        ]
    }]
}